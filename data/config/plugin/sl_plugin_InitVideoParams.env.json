[
    { "--": "--" },

    { "__import": "last_played_file" },

    { "temp_cmd_out_file": "$(sl_slang_db_temp_dir)/command_output.tmp"},


    { "--": "mkv format" },
    { "reg_mkv_source": "^(.*\\.mkv)$" },
    { "pre_mkv_command": ["ffprobe", "-i"] },
    
    { "--": "    Stream #0:0: Video: h264 (High), yuv420p(progressive), ..." },
    { "pre_mkv_reg_videos": "Stream #(\\d+:\\d+)(([^ ]+)?: Video)(((?!Stream|\\\\r\\\\n|\\\\n|$).)*)(?:\\\\r\\\\n|$)([ ]{0,}Metadata\\:(?:\\\\r\\\\n|\\\\n|$)[ ]{0,}title[ ]{0,}: (((?!\\\\r\\\\n|\\\\n|$).)*)(\\\\r\\\\n|$))?" },
    { "pre_mkv_reg_audios": "Stream #(\\d+:\\d+)(([^ ]+)?: Audio)(((?!Stream|\\\\r\\\\n|\\\\n|$).)*)(?:\\\\r\\\\n|$)([ ]{0,}Metadata\\:(?:\\\\r\\\\n|\\\\n|$)[ ]{0,}title[ ]{0,}: (((?!\\\\r\\\\n|\\\\n|$).)*)(\\\\r\\\\n|$))?" },
    { "pre_mkv_reg_subtitles": "Stream #(\\d+:\\d+)(([^ ]+)?: Subtitle)(((?!Stream|\\\\r\\\\n|\\\\n|$).)*)(?:\\\\r\\\\n|$)([ ]{0,}Metadata\\:(?:\\\\r\\\\n|\\\\n|$)[ ]{0,}title[ ]{0,}: (((?!\\\\r\\\\n|\\\\n|$).)*)(\\\\r\\\\n|$))?" },


    { "ffmpeg_executable": "ffmpeg" },

    { "--": "extract audio, command like: " },
    { "--": "ffmpeg -i my_file.mkv -map 0:8 -b:a 64k -ac 2 out.mp3 " },
    { "input_audio_file_option": "-i" },
    { "input_audio_file_value": "$(last_played_file)" },
    { "input_audio_map_option": "-map" },
    { "input_audio_map_value": "" },
    { "bitrate_audio_option": "-b:a" },
    { "bitrate_audio_value": "64k" },
    { "channels_audio_option": "-ac" },
    { "channels_audio_value": "2" },
    { "output_audio_file_value": "" },

    { "audio_mkv_command_param_names": [
    	"ffmpeg_executable",
    	"input_audio_file_option", "input_audio_file_value",
    	"input_audio_map_option", "input_audio_map_value",
    	"bitrate_audio_option", "bitrate_audio_value",
    	"channels_audio_option", "channels_audio_value",
    	"output_audio_file_value"
    ] },


    { "--": "extract subtitles, command like: " },
    { "--": "ffmpeg -i my_file.mkv -map 0:10 en.srt " },
    { "input_subtitle_file_option": "-i" },
    { "input_subtitle_file_value": "$(last_played_file)" },
    { "input_subtitle_map_option": "-map" },
    { "input_subtitle_map_value": "" },
    { "output_subtitle_file_value": "" },

    { "subtitle_mkv_command_param_names": [
    	"ffmpeg_executable",
    	"input_subtitle_file_option", "input_subtitle_file_value",
    	"input_subtitle_map_option", "input_subtitle_map_value",
    	"output_subtitle_file_value"
    ] },


    { "__export": "last_position" },
    { "__export": "video_audio_track" },
    { "__export": "video_audio_file" },
    { "__export": "video_subt1_file" },
    { "__export": "video_subt2_file" },

    { "--": "--" }
]